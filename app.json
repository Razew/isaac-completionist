{
  "expo": {
    "name": "isaac-completionist",
    "slug": "isaac-completionist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash_text.png",
      "resizeMode": "contain",
      "backgroundColor": "#996AD1"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#996AD1"
      },
      "package": "com.razew.isaaccompletionist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "LANDSCAPE"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ae014f5f-3758-425a-8352-cac1b57efb4f"
      }
    }
  }
}
